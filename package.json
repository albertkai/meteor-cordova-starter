{
  "name": "meteor-cordova-starter",
  "private": true,
  "scripts": {
    "start": "npm install && meteor run --settings settings.json",
    "deploy": "meteor deploy <url> --settings settings.json",
    "device": "meteor run ios-device --settings settings.json",
    "sim": "npm install && meteor run ios --settings settings.json",
    "build": "meteor build ../app-build --server https://example.server.com --mobile-settings settings.json"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcrypt": "^0.8.7",
    "immutable": "^3.8.1",
    "meteor-node-stubs": "~0.2.4",
    "moment": "^2.18.1",
    "node-onesignal": "^0.3.1",
    "react": "^15.4.2",
    "react-addons-perf": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.4.1",
    "react-fastclick": "^3.0.2",
    "react-mounter": "^1.2.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-active-component": "^4.0.0",
    "react-textarea-autosize": "^5.1.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "babel-plugin-transform-class-properties": "^6.24.1"
  }
